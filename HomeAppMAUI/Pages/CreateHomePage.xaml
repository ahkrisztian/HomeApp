<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HomeAppMAUI.Pages.CreateHomePage"
             Title="Create Home">
    <VerticalStackLayout>
        <Entry Text="{Binding NewHomeModel.Name}" Placeholder="Enter Name"/>
        <Entry Text="{Binding NewHomeModel.Description}" Placeholder="Enter AddressId"/>
        <Entry Text="{Binding NewHomeModel.UserId}" Placeholder="Enter AddressId"/>
        <Entry Text="{Binding NewHomeModel.AddressId}" Placeholder="Enter AddressId"/>
        <Button Text="Create" Clicked="OnSaveButtonClicked"/>
        <Button Text="Cancel" Clicked="OnCancelButtonClicked"/>
    </VerticalStackLayout>
</ContentPage>